4_a) procedure posicion (v:vector;var pos,numX:integer);
     var
       dimL:integer;
     begin
       dimL:=1;
       while (numX<> v[dimL]) do begin
         dimL:=dimL+1;
         pos:=-1;
        end;
       if (numX=v[dimL] then
         pos:=dimL;
       end;

